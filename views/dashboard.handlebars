<h2>Welcome {{name}}</h2>

<p>You have written {{posts.length}} posts so far</p>

<div id="post-container">
{{#each posts as |post|}}
<div class="card">
    <div class="card-body">
        <h4 class="card-title">{{post.title}}</h4>
        <p class="card-text content-partial">{{post.content}}</p>
        <a href="/post/{{post.id}}" class="btn btn-primary">See Post</a>
    </div>
</div>
{{/each}}
</div>
<script src="./js/logout.js"></script>